      <LEXMeanings>
        <LEXMeaning Id="001149001001000" IsBiblicalTerm="M" EntryCode="9.34" Indent="0">
          <LEXDomains>
            <LEXDomain>People</LEXDomain>
          </LEXDomains>
          <LEXSubDomains>
