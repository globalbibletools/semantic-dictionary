        <LEXMeaning Id="003698001001000" IsBiblicalTerm="M" EntryCode="11.1" Indent="0">
          <LEXDomains>
            <LEXDomain>Groups and Classes of Persons and Members of Such Groups and Classes</LEXDomain>
