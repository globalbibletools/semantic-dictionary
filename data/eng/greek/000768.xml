        <LEXMeaning Id="000768001002000" IsBiblicalTerm="M" EntryCode="88.117" Indent="0">
          <LEXDomains>
            <LEXDomain>Moral and Ethical Qualities and Related Behavior</LEXDomain>
          </LEXDomains>
