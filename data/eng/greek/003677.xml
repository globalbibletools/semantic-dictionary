        <LEXMeaning Id="003677001005000" IsBiblicalTerm="Y" EntryCode="4.41" Indent="0">
          <LEXDomains>
            <LEXDomain>Animals</LEXDomain>
          </LEXDomains>
          <LEXSubDomains>
