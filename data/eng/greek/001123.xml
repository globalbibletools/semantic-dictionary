        <LEXMeaning Id="001123001004000" IsBiblicalTerm="Y" EntryCode="28.57" Indent="0">
          <LEXDomains>
            <LEXDomain>Know</LEXDomain>
          </LEXDomains>
          <LEXSubDomains>
