      <LEXMeanings>
        <LEXMeaning Id="002083001001000" IsBiblicalTerm="M" EntryCode="23.1" Indent="0">
          <LEXDomains>
            <LEXDomain>Physiological Processes and States</LEXDomain>
          </LEXDomains>
          <LEXSubDomains>
