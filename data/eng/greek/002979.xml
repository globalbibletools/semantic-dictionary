      <LEXMeanings>
        <LEXMeaning Id="002979001001000" IsBiblicalTerm="M" EntryCode="12.9{N:001}" Indent="0">
          <LEXDomains>
            <LEXDomain>Supernatural Beings and Powers</LEXDomain>
          </LEXDomains>
