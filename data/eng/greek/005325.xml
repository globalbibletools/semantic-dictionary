        <LEXMeaning Id="005325001003000" IsBiblicalTerm="M" EntryCode="67.196" Indent="0">
          <LEXDomains>
            <LEXDomain>Time</LEXDomain>
          </LEXDomains>
          <LEXSubDomains>
