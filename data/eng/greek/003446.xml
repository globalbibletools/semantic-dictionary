        <LEXMeaning Id="003446001001000" IsBiblicalTerm="Y" EntryCode="31.29" Indent="0">
          <LEXDomains>
            <LEXDomain>Hold a View, Believe, Trust</LEXDomain>
          </LEXDomains>
          <LEXSubDomains>
