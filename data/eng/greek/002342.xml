        <LEXMeaning Id="002342001002000" IsBiblicalTerm="Y" EntryCode="23.8" Indent="0">
          <LEXDomains>
            <LEXDomain>Physiological Processes and States</LEXDomain>
          </LEXDomains>
          <LEXSubDomains>
