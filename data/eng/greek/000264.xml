        <LEXMeaning Id="000264001003000" IsBiblicalTerm="M" EntryCode="88.310" Indent="0">
          <LEXDomains>
            <LEXDomain>Moral and Ethical Qualities and Related Behavior</LEXDomain>
          </LEXDomains>
          <LEXSubDomains>
